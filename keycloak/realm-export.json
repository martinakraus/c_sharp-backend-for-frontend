{
  "realm": "bff-realm",
  "enabled": true,
  "sslRequired": "none",
  "registrationAllowed": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,
  "bruteForceProtected": true,
  "accessTokenLifespan": 120,
  "defaultSignatureAlgorithm": "RS256",
  "revokeRefreshToken": true,
  "refreshTokenMaxReuse": 1,
  "roles": {
    "realm": [
      {
        "name": "offline_access",
        "description": "Offline Access",
        "composite": false,
        "clientRole": false
      },
      {
        "name": "users:read",
        "description": "Read access to user resources",
        "composite": false,
        "clientRole": false
      },
      {
        "name": "users:write",
        "description": "Write access to user resources",
        "composite": false,
        "clientRole": false
      }
    ]
  },
  "defaultRoles": ["offline_access"],
  "clients": [
    {
      "clientId": "bff-client",
      "name": "BFF Application Client",
      "description": "Confidential client for Backend-for-Frontend with PKCE flow",
      "surrogateAuthRequired": false,
      "enabled": true,
      "alwaysDisplayInConsole": false,
      "clientAuthenticatorType": "client-secret",
      "secret": "bff-secret",
      "redirectUris": [
        "http://localhost:3000/auth/callback",
        "http://localhost:3000/*",
        "http://localhost:4200/*"
      ],
      "webOrigins": [
        "http://localhost:3000"
      ],
      "notBefore": 0,
      "bearerOnly": false,
      "consentRequired": false,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "publicClient": false,
      "frontchannelLogout": false,
      "protocol": "openid-connect",
      "attributes": {
        "realm_client": "false",
        "oidc.ciba.grant.enabled": "false",
        "backchannel.logout.session.required": "true",
        "post.logout.redirect.uris": "http://localhost:3000/*##http://localhost:4200/*",
        "oauth2.device.authorization.grant.enabled": "false",
        "pkce.code.challenge.method": "S256",
        "backchannel.logout.revoke.offline.tokens": "false",
        "login_theme": "",
        "display.on.consent.screen": "false",
        "consent.screen.text": "",
        "backchannel.logout.url": ""
      },
      "authenticationFlowBindingOverrides": {},
      "fullScopeAllowed": true,
      "nodeReRegistrationTimeout": -1,
      "protocolMappers": [
        {
          "name": "audience-mapper",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-audience-mapper",
          "consentRequired": false,
          "config": {
            "included.client.audience": "account",
            "id.token.claim": "false",
            "access.token.claim": "true",
            "userinfo.token.claim": "false"
          }
        },
        {
          "name": "realm-roles-mapper",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-realm-role-mapper",
          "consentRequired": false,
          "config": {
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "realm_access.roles",
            "jsonType.label": "String",
            "multivalued": "true",
            "userinfo.token.claim": "true"
          }
        },
        {
          "name": "client-roles-mapper",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-client-role-mapper",
          "consentRequired": false,
          "config": {
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "resource_access.${client_id}.roles",
            "jsonType.label": "String",
            "multivalued": "true",
            "userinfo.token.claim": "true"
          }
        }
      ],
      "defaultClientScopes": [
        "openid",
        "profile",
        "offline_access"
      ],
      "access": {
        "view": true,
        "configure": true,
        "manage": true
      },
      "rootUrl": "",
      "baseUrl": "",
      "adminUrl": "",
      "authorizationServicesEnabled": false
    }
  ],
  "users": [
    {
      "username": "testuser",
      "enabled": true,
      "email": "testuser@example.com",
      "emailVerified": true,
      "firstName": "Test",
      "lastName": "User",
      "credentials": [
        {
          "type": "password",
          "value": "password",
          "temporary": false
        }
      ],
      "realmRoles": [
        "offline_access",
        "users:read",
        "users:write"
      ]
    },
    {
      "username": "readonly",
      "enabled": true,
      "email": "readonly@example.com",
      "emailVerified": true,
      "firstName": "Read",
      "lastName": "Only",
      "credentials": [
        {
          "type": "password",
          "value": "password",
          "temporary": false
        }
      ],
      "realmRoles": [
        "offline_access",
        "users:read"
      ]
    }
  ],
  "clientScopes": [
    {
      "id": "9e08edbf-b386-4322-b4d9-733a04d47904",
      "name": "offline_access",
      "description": "OpenID Connect built-in scope: offline_access",
      "protocol": "openid-connect",
      "attributes": {
        "consent.screen.text": "${offlineAccessScopeConsentText}",
        "display.on.consent.screen": "true"
      }
    },
    {
      "id": "10231fe1-a056-4a9b-911b-e866a379c705",
      "name": "profile",
      "description": "",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "false",
        "display.on.consent.screen": "true",
        "gui.order": "",
        "consent.screen.text": ""
      }
    },
    {
      "id": "30bee013-ff70-4790-8aa8-d613ec359cd1",
      "name": "user-scope",
      "description": "User management scope",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "true",
        "display.on.consent.screen": "true"
      }
    },
    {
      "id": "31d37807-1f73-4cc3-adf8-cd9972e6ddfe",
      "name": "openid",
      "description": "",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "false",
        "display.on.consent.screen": "true",
        "gui.order": "",
        "consent.screen.text": ""
      }
    }
  ],
  "defaultDefaultClientScopes": [],
  "defaultOptionalClientScopes": [
    "offline_access",
    "openid",
    "profile",
    "user-scope"
  ]
}
